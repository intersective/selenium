<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite - Practera Review" parallel="none" preserve-order="true">

	<listeners>
		<listener class-name="listener.InitPEEnvironmentListener"/>
	</listeners>
	
	<test name="Practera Review test" group-by-instances="true" preserve-order="true">
		<classes>
	  		<class name="testsuit.practera.TestReview" />
	  		<class name="testsuit.practera.TestMentorReview" />
	  		<class name="testsuit.practera.TestPublishReview" />
	  	</classes>
	</test>

</suite>